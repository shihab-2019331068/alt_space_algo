ll ret_dis(ll u,ll v)
{
   ll l,r,h1,h2,h,d;
   l=F[u]; r=F[v];
   if(l>r) swap(l,r);
   h=rnq(l,r); h1=a[l]; h2=a[r];
   d=h1+h2-h*2;
   return d;
}
 
void dfs(ll u=1,ll p=0,ll h=0)
{
   a[++I]=h;
   if(!F[u]) F[u]=I;
   for(ll v:g[u]){
      if(v==p) continue;
      dfs(v,u,h+1);
      a[++I]=h;
   }
}|
